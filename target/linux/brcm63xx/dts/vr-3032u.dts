&sleds0 {
	status = "ok";

	inet_red {
		compatible = "serial-led";
		pin = <0x2>; /* BP_SERIAL_GPIO_2 */
		label = "VR-3032u:red:inet";
		active_low;
	};
	dsl_green {
		compatible = "serial-led";
		pin = <0x3>; /* BP_SERIAL_GPIO_3 */
		label = "VR-3032u:green:dsl";
		active_low;
	};
	usb_green {
		compatible = "serial-led";
		pin = <0x4>; /* BP_SERIAL_GPIO_4 */
		label = "VR-3032u:green:usb";
		active_low;
	};
	wps_green {
		compatible = "serial-led";
		pin = <0x7>; /* BP_SERIAL_GPIO_7 */
		label = "VR-3032u:green:wps";
		active_low;
	};
};

&sleds1 {
	status = "ok";

	inet_green {
		compatible = "serial-led";
		pin = <0x0>; /* BP_SERIAL_GPIO_8 */
		label = "VR-3032u:green:inet";
		active_low;
	};
	lan2_green {
		compatible = "serial-led";
		pin = <0x1>; /* BP_SERIAL_GPIO_9 */
		label = "VR-3032u:green:lan2";
		active_low;
	};
	lan3_green {
		compatible = "serial-led";
		pin = <0x2>; /* BP_SERIAL_GPIO_10 */
		label = "VR-3032u:green:lan3";
		active_low;
	};
	lan4_green {
		compatible = "serial-led";
		pin = <0x3>; /* BP_SERIAL_GPIO_11 */
		label = "VR-3032u:green:lan4";
		active_low;
	};
	lan1_green {
		compatible = "serial-led";
		pin = <0x4>; /* BP_SERIAL_GPIO_12 */
		label = "VR-3032u:green:lan1";
		active_low;
	};
};
