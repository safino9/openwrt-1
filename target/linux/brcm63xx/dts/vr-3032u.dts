	gpio-keys-polled {
		compatible = "gpio-keys-polled";
		#address-cells = <1>;
		#size-cells = <0>;
		poll-interval = <20>;
		debounce-interval = <60>;

		reset {
			label = "reset";
			gpios = <&gpio1 0 1>;
			linux,code = <0x198>;
		};
		wps {
			label = "wps";
			gpios = <&gpio1 1 1>;
			linux,code = <0x211>;
		};
	};

	gpio-leds {
		compatible = "gpio-leds";

		power_green {
			label = "VR-3032u:green:power";
			gpios = <&gpio0 20 1>;
			default-state = "on";
		};
